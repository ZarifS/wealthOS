{
  "name": "wealth-x",
  "version": "1.0.0",
  "description": "Server for Wealth-X using Plaid, MongoDB and Express",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZarifS/wealth-x.git"
  },
  "keywords": [
    "Plaid",
    "ExpressJS",
    "MongoDB"
  ],
  "author": "Zarif Shahriar",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ZarifS/wealth-x/issues"
  },
  "homepage": "https://github.com/ZarifS/wealth-x#readme",
  "main": "api/app.js",
  "scripts": {
    "start": "nodemon --exec babel-node api/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.6",
    "morgan": "^1.9.1",
    "plaid": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0"
  }
}
